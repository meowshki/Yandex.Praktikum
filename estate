    "def normalize_locality_name(value):\n",
    "    if str(value) == 'nan':\n",
    "        return\n",
    "    \n",
    "    locality_name = ['–ú—É—Ä–∏–Ω–æ', '–ö—É–¥—Ä–æ–≤–æ']\n",
    "    words = value.split(' ')\n",
    "    \n",
    "    if words[0] == '–ø–æ—Å—ë–ª–æ–∫':\n",
    "        words[0] = '–ø–æ—Å–µ–ª–æ–∫'\n",
    "    \n",
    "    for locality in locality_name:\n",
    "        if locality in words:\n",
    "            return locality\n",
    "        \n",
    "    return ' '.join(words)"
   ]
